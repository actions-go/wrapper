{
  "name": "golang-action",
  "version": "0.0.0",
  "private": true,
  "description": "golang template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "golang",
    "go",
    "setup"
  ],
  "author": "Thibault Jamet",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.2",
    "@actions/exec": "^1.0.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^13.9.4",
    "@typescript-eslint/parser": "^3.7.0",
    "@zeit/ncc": "^0.21.1",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^23.8.1",
    "jest": "^25.1.0",
    "jest-circus": "^25.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "typescript": "^3.8.3"
  }
}
